{
    "name": "fulll/security-bundle",
    "description": "Signed requests check",
    "keywords": ["security", "symfony", "bundle"],
    "type": "symfony-bundle",
    "homepage": "https://github.com/fulll/security-bundle",
    "license": "MIT",
    "authors": [{
        "name": "Community contributors",
        "homepage": "https://github.com/fulll/security-bundle/contributors"
    }],
    "require": {
        "php": ">=8.0",
        "symfony/framework-bundle": "~5.4|~6.0",
        "symfony/security-bundle" : "~5.4|~6.0",
        "doctrine/common": "~2.2|~3.0"
    },
    "require-dev": {
        "atoum/atoum": "~4.0"
    },
    "suggest": {
        "psr/http-message": "Required by \\Rezzza\\SecurityBundle\\Request\\Psr7RequestSigner"
    },
    "autoload":     {
        "psr-0": { "Rezzza\\SecurityBundle": "" }
    },
    "target-dir": "Rezzza/SecurityBundle",
    "extra": {
        "branch-alias": {
            "dev-master": "2.0.x-dev"
        }
    },
    "config": {
        "bin-dir": "bin"
    }
}
